def name_list_with_lengths():
    names = []

    print("Enter 5 names:")

    for i in range(5):
        name = input(f"Name {i + 1}: ")
        names.append(name)

    print("\n--- Name List with Lengths ---")
    for name in names:
        print(f"{name} (Length: {len(name)})")

# Run the function
name_list_with_lengths()
